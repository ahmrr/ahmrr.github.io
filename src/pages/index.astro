---
import Layout from "@layouts/Layout.astro";
import Navbar from "@components/Navbar.astro";
import ProfileSection from "@components/sections/index/ProfileSection.astro";
import AboutSection from "@components/sections/index/AboutSection.astro";
import FeaturedProjectsSection from "@components/sections/index/FeaturedProjectsSection.astro";
import SkillsSection from "@components/sections/index/SkillsSection.astro";
import FinalSection from "@components/sections/FinalSection.astro";
---

<Layout title="Ahmer Raza">
    <div
        class="min-w-[535px] max-w-screen-md mx-auto py-16 px-8 dark:selection:bg-gray-600 dark:selection:text-white"
    >
        <header>
            <Navbar />
            <ProfileSection />
        </header>
        <main>
            <AboutSection />
            <FeaturedProjectsSection />
            <!-- <SkillsSection /> -->
        </main>
        <footer>
            <FinalSection />
        </footer>
    </div>
</Layout>
