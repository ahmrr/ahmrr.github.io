*,:after,:before{box-sizing:border-box;padding:0}body{font:1rem/1.5 inter,-apple-system,BlinkMacSystemFont,avenir next,avenir,helvetica,helvetica neue,ubuntu,roboto,noto,segoe ui,arial,sans-serif;text-rendering:optimizeLegibility;-webkit-font-smoothing:antialiased;-moz-osx-font-smoothing:grayscale;margin:0;padding:2rem 2rem 0;background:#f5f5f5;color:#3a343a;display:flex;flex-direction:column;justify-content:start;min-height:100vh;max-width:70ch;margin:0 auto}h1{font-size:x-large}h2{font-size:large}h1,h2,h3,h4,h5,h6{user-select:none}header{line-height:2;margin-bottom:1rem}header h1{font-size:xx-large}header a{text-decoration:none}header ul{list-style-type:none;padding:0}header ul li{padding-left:1rem;display:inline}header ul li a{font-size:large}footer{display:flex;width:calc(100% - $edge);margin:0 -2rem;justify-content:center;align-items:center;flex-direction:column;margin-top:auto}a{color:inherit}a:hover{text-decoration:none}time{font-variant-numeric:tabular-nums;white-space:nowrap}blockquote{border-left:.25rem solid #ccc;padding-left:1rem;margin:0}pre{padding:.5rem;overflow:auto;overflow-wrap:normal}ul{list-style-type:square}ul,ul ol{padding-left:1.2rem}hr{margin:auto .5rem;height:1px;border:0;border-bottom:1px solid #ccc;flex:1}img{max-width:100%;height:auto}code:not(.highlight *){background:#eee;border:1px solid #ccc;padding:2px;border-radius:4px}div.highlight{overflow-x:auto;overflow-y:auto;margin:1rem 0;width:100%}div.highlight>div{border-radius:2px;width:100%}.util__p-0{padding:0}.util__m-0{margin:0}.util__flex{display:flex}.util__flex-center{display:flex;justify-content:center;align-items:center}.img__inline{display:inline-block;height:20px;width:20px;color:#f5f5f5}.img__profile{display:inline;float:right;width:150px;height:150px;margin:16px;border-radius:50%}.media__panel{display:flex;justify-content:center;align-items:center;gap:10px;user-select:none}.media__button{display:flex;width:fit-content;height:fit-content;padding:8px;border-radius:50%;background:#1e1e2e;cursor:pointer;transition:ease .2s}.media__button:hover{transform:rotate(360deg);border-radius:50%}.link__skip{position:absolute;top:-40px;left:0;background:0 0;z-index:999}.link__skip:focus{top:0}.posts__list{line-height:2;list-style-type:none;padding-left:0}.posts__list li{padding-bottom:.1rem;display:flex;align-items:baseline}.header__content{display:flex;justify-content:space-between;align-items:center;margin-bottom:16px}.header__title{font-size:xx-large}@keyframes progress{}.progress__wrapper{display:flex;justify-content:center;align-items:center;width:5rem;height:5rem;border-radius:50%;user-select:none}.progress__element{visibility:hidden;height:0;width:0}